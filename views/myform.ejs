<%- include('bootheader') -%>
<%- include('menubar') -%>
    <div class="jumbotron">
        <h1>New Post</h1>
    </div>

    <form method="post" action="/processform">
      Title: <input type="text" name="url"> <br> <br>
      <textarea name="comment" rows="40" cols="150"></textarea>
      <% if (loggedIn) { %>
      <input type="submit">
      <% } else { %>
        You must be logged in to submit form info.
        <% } %>
    </form>

